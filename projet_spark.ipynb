   "source": [
    "for i in range(0,len(tweets_df)):\n",
    "    s = tweets_df.iloc[i][\"source media\"]\n",
    "    start = s.find(\">\") + len(\"6\")\n",
    "    end = s.find(\"|</a>\")\n",
    "    substring = s[start:end]\n",
    "    substring = substring[:-3]\n",
    "    tweets_df.at[i,'source media']=substring"
   ]
